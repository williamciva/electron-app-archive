{
  "name": "electron-app-archive",
  "version": "1.0.0",
  "description": "a simple app for generate file in ERP Sankhya",
  "main": "build/electron/index.js",
  "scripts": {
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",

    "start:electron": "npm run build:electron && electron-forge start",
    "build:electron": "rimraf ./build && tsc",
    "package:electron": "npm run build:electron && electron-forge package",
    "make:electron": "electron-forge make",

    "dev": "npm-run-all --parallel start:react start:electron",

    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "William Civa",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@types/node": "^18.13.0",
    "electron": "^23.0.0",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
